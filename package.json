{
  "name": "create-json-public-tagmanager-container",
  "displayName": "create-json-public-tagmanager-container",
  "description": "Procurara dentro do projetos as diretivas que foram criadas para gerar um json que servira como o arquivo import para enviar ao tagmanger",
  "version": "1.0.5",
  "publisher": "create-json-public-tagmanager-container",
  "icon": "logo/logo_ajuda.png",
  "engines": {
    "vscode": "^1.99.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juanalemau/create-json-public-tagmanager-container"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "create-json-public-tagmanager-container.gerarJsonTagmanager",
        "title": "Gerar Json Tag Manager"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  }
}
